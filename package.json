{
  "name": "vislite-examples",
  "description": "基于VISLite及相关产品开发的一些可以快速复制的例子代码",
  "scripts": {
    "postinstall": "npm run copy:vislite && node ./build/install.js",
    "dev": "webpack server --config ./build/webpack.dev.js",
    "build": "oipage-cli disk -d ./docs/dist && webpack --config ./build/webpack.prod.js",
    "copy:vislite": "oipage-cli disk -c ./node_modules/vislite/lib ./docs/cache/vislite -f"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rapid-start/VISLite-examples.git"
  },
  "keywords": [
    "project",
    "webpack",
    "zipaper",
    "vislite",
    "@vislite/canvas",
    "@vislite/chart"
  ],
  "author": {
    "name": "zxl20070701",
    "url": "https://zxl20070701.github.io/notebook/home.html"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rapid-start/VISLite-examples/issues"
  },
  "homepage": "https://github.com/rapid-start/VISLite-examples",
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "babel-loader": "^9.1.0",
    "css-loader": "^7.1.2",
    "html-webpack-plugin": "^5.5.0",
    "oipage": "^1.5.0",
    "sass": "^1.86.0",
    "sass-loader": "^16.0.5",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "xhtml-to-json": "^0.1.0"
  },
  "dependencies": {
    "pointin": "^0.1.1",
    "vislite": "^1.7.0",
    "zipaper": "^0.2.1"
  }
}